.our-story-banner {
  background-image: url('https://cdn.shopify.com/s/files/1/0577/7129/4793/t/6/assets/ourstory-ban.jpg');
  height: 100%;
  background-size: cover;
}
.our-story-banner h1 {
  font-weight: 700;
  font-size: 54px;
  line-height: 130.8%;
  position: relative;
  text-align: center;
  color: #fff;
  padding: 16% 0;
}
.our-story-sec-2 {
  background-color: var(--dark);
  height: 39vh;
  display: flex;
  align-items: center;
  padding: 0 23%;
}
.our-story-sec-2 .para {
  color: #fff;
  font-size: 54px;
  font-weight: 700;
}
div#moneyback .black-box {
  height: 335px;
}
ul li:before {
  background-image: none;
}
.our-story-sec-3 .nav-tabs .nav-link {
  font-weight: 700;
  font-size: 28px;
}
.our-story-sec-3 .nav-link {
  color: #ACACAC;
}
.our-story-sec-3 ul li {
  margin-right: 26px;
}
.our-story-sec-3 .tab-content p,
.fs-pa {
  font-size: 20px;
}
.our-story-sec-3 .nav-tabs .nav-link.active {
  border-color: #ffffff #ffffff #000;
  border-bottom: 3px solid !important;
}
.our-story-sec-3 .nav-tabs .nav-link,
.our-story-sec-3 .nav-tabs .nav-link:hover {
  border-color: #fff;
}
.w-20 {
  width: 40%;
  height: 100px;
  object-fit: contain;
}
.black-box {
  background-color: var(--dark);
  color: #fff;
  border-radius: 25px;
}
.sub-para {
  font-size: 54px;
  font-weight: 400;
}
.glide__arrow--prev {
    top: -38px;
    right: 50px;
}

.glide__arrow--next {
    top: -38px;
    right: 0;
}
.glide__arrow {
    background: #000;
}

.glide__arrow svg {
    fill: #fff;
}
