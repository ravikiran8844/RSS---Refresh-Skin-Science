/******** Animation Css********/
@keyframes marqueeRight {
  0% {
    left: 0;
  }
  100% {
    left: -100%;
  }
}
@keyframes marqueeMobileRight {
  0% {
    transform: translate3d(0, 0, 0);
  }
  100% {
    transform: translate3d(-300%, 0, 0);
  }
}
@keyframes slide-right {
  from {
    opacity: 1;
    visibility: visible;
    transform: translateX(50%);
  }
  to {
    opacity: 1;
    visibility: visible;
    transform: translateX(0%);
  }
}
@keyframes bubbler {
  0% {
    transform: scale(0) translateY(0);
  }
  50% {
    transform: scale(20) translateY(-15%);
  }
  80% {
    transform: scale(28) translateY(-20%);
  }
  100% {
    transform: scale(30) translateY(-20%);
  }
}
@keyframes slide_up {

  from {

    color: #fff;
    transform: translate3d(0, 100%, 0);
  }
  to {
    visibility: visible;
    color: #fff;
    transform: translate3d(0, 0, 0);
  }

}
@keyframes slide_up_mobile {
  from {

    color: #666;
    transform: translate3d(0, 100%, 0);
  }
  to {
    visibility: visible;
    color: #666;
    transform: translate3d(0, 0, 0);
  }
}
@keyframes slide_up_li {

  from {

    color: #7c7c7c;
    transform: translate3d(0, 100%, 0);
  }
  to {
    visibility: visible;
    color: #7c7c7c;
    transform: translate3d(0, 0, 0);
  }

}

@keyframes bubbler_close {
  0% {
    transform: scale(30) translateY(-20%);
    opacity: 1;
    visibility: visible;
  }
  50% {
    transform: scale(23) translateY(-25%);
    opacity: 1;
    visibility: visible;
  }
  80% {
    transform: scale(15) translateY(-15%);
    opacity: 1;
    visibility: visible;
  }
  100% {
    transform: scale(0) translateY(0);
    opacity: 0;
    visibility: hidden;
  }
}
@keyframes rotate {
  0% {
    transform: rotate(0deg);
  }
  100% {
    transform: rotate(360deg);
  }
}


