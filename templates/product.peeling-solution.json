        },
        "b6f0e51e-9645-4913-865a-0a5733cadb3d": {
          "type": "how-to-use-steps",
          "settings": {
            "hw_to_use_link": "https:\/\/cdn.shopify.com\/videos\/c\/o\/v\/9843daa4a51d472eaf1b8eb923d46393.mp4",
            "hw_to_use_step": "Step 02",
            "hw_to_use_det": "Apply peeling solution evenly across face and neck area and gently massage evenly using fingertips, avoiding the eye area. Leave on for no more than 10 minutes. Rinse thoroughly with lukewarm water."
          }
        },
        "cd325ac8-9b4a-4762-838c-f06f521f859e": {
          "type": "how-to-use-steps",
          "settings": {
            "hw_to_use_link": "https:\/\/cdn.shopify.com\/videos\/c\/o\/v\/dbc1062b6c504aafbbb6862452417e72.mp4",
            "hw_to_use_step": "Step 03",
            "hw_to_use_det": "Follow your normal skincare routine after a peeling session for maximum absorption and efficiency, and never step out without sun protection."
          }
        }
