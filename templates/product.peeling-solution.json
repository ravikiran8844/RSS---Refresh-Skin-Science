    "product-faq": {
      "type": "product-faq",
      "blocks": {
        "7d009913-62e9-4feb-8830-e06312699731": {
          "type": "faq-sec-accordion",
          "settings": {
            "faq_question": "How often should I deep cleanse my skin?",
            "faq_answers": "The peeling solution can be used only once a week. Therefore, it is advisable to use a gentle cleansing foam to cleanse your skin daily."
          }
        },
        "13b80d15-b19a-4834-90f9-0fe936a408b2": {
          "type": "faq-sec-accordion",
          "settings": {
            "faq_question": "Is continuous usage necessary?",
            "faq_answers": "Consistent usage helps prevent clogging of pores and promotes skin renewal giving your face a youthful, dewy appearance."
          }
        },
        "de17f659-ad7a-4e80-8887-42ce89bf4412": {
          "type": "faq-sec-accordion",
          "settings": {
            "faq_question": "Does the peeling solution shrink my pores?",
            "faq_answers": "Pore size cannot be altered and is entirely dependent on your genetics. Usage of the peeling solution will only prevent your pores from clogging and accumulating excess oil\/sebum."
          }
        },
        "69c9a38c-e6a6-439a-ae7f-985680ec1457": {
          "type": "faq-sec-accordion",
          "settings": {
            "faq_question": "Can I go out after a peeling session?",
            "faq_answers": "It is not advisable to go into direct sunlight after a peeling session as your new skin is susceptible to sunburn and sun damage. In any unavoidable situation, use strong sunscreen before heading out."
          }
        },
        "965fe1c9-79e2-4708-9a4f-b1a408374137": {
          "type": "faq-sec-accordion",
          "settings": {
            "faq_question": "Is it safe to use other skincare products after the peeling solution?",
            "faq_answers": "Yes, it is considered the best option to moisturize after you apply the peeling solution as it reduces your risk of any irritation on the skin and forms a protective barrier."
          }
        }
      },
