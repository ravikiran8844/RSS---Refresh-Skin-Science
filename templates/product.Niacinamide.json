    "product-faq": {
      "type": "product-faq",
      "blocks": {
        "7d009913-62e9-4feb-8830-e06312699731": {
          "type": "faq-sec-accordion",
          "settings": {
            "faq_question": "At what age is it recommended to start using?",
            "faq_answers": "20 years is a perfect time to kickstart your skincare journey when your body’s natural ability to protect its skin barriers slowly starts to degrade. It can also help immensely to reduce acne and prevent it from scarring."
          }
        },
        "13b80d15-b19a-4834-90f9-0fe936a408b2": {
          "type": "faq-sec-accordion",
          "settings": {
            "faq_question": "Will it reduce my acne?",
            "faq_answers": "Niacinamide Zinc Serum has inflammatory properties and controls excess sebum production to a great extent which helps with acne. It can also help with acne marks and dark brown spots."
          }
        },
        "de17f659-ad7a-4e80-8887-42ce89bf4412": {
          "type": "faq-sec-accordion",
          "settings": {
            "faq_question": "Will my skin feel sticky post application?",
            "faq_answers": "Our lightweight, oil-free formula ensures your skin feels light and supple post-application without leaving a sticky residue on the face."
          }
        },
        "69c9a38c-e6a6-439a-ae7f-985680ec1457": {
          "type": "faq-sec-accordion",
          "settings": {
            "faq_question": "Will Niacinamide be suitable for a combination skin type?",
            "faq_answers": "Yes, Niacinamide is perfectly suitable for combination skin types and can reduce your skin's dryness when it's dry and help with retaining moisture."
          }
        },
        "965fe1c9-79e2-4708-9a4f-b1a408374137": {
          "type": "faq-sec-accordion",
          "settings": {
            "faq_question": "Can I use this serum along with Vitamin C serum?",
            "faq_answers": "If topical Vitamin C is used as a part of the skincare routine, it is always recommended that you use Niacinamide alternatively with Vitamin C."
          }
        }
      },
