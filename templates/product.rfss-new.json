    "product-faq": {
      "type": "product-faq",
      "blocks": {
        "7d009913-62e9-4feb-8830-e06312699731": {
          "type": "faq-sec-accordion",
          "settings": {
            "faq_question": "How is Hyaluronic acid different from other acids like Salicylic acid?",
            "faq_answers": "Acids, in general, are associated with “stripping,” Hyaluronic acid is a natural humectant and helps draw moisture to your skin, keeping it hydrated and nourished for smoother and younger-looking skin."
          }
        },
        "13b80d15-b19a-4834-90f9-0fe936a408b2": {
          "type": "faq-sec-accordion",
          "settings": {
            "faq_question": "Do I have to use it daily?",
            "faq_answers": "Yes! Consistency is key to any skincare routine, and it is recommended that you use our best hyaluronic acid serum as a part of your AM and PM routine."
          }
        },
        "de17f659-ad7a-4e80-8887-42ce89bf4412": {
          "type": "faq-sec-accordion",
          "settings": {
            "faq_question": "Can Hyaluronic acid be used with other acids?",
            "faq_answers": "Hyaluronic acids go well with most ingredients and is perfect for layering as it acts as a deep moisturizer."
          }
        },
        "69c9a38c-e6a6-439a-ae7f-985680ec1457": {
          "type": "faq-sec-accordion",
          "settings": {
            "faq_question": "Is hyaluronic acid good for all skin types?",
            "faq_answers": "Hyaluronic acid is great for all skin types! While it is specifically recommended for people with dry, mature skin, everyone can reap the benefits by indulging their skin in some hydration!"
          }
        },
        "965fe1c9-79e2-4708-9a4f-b1a408374137": {
          "type": "faq-sec-accordion",
          "settings": {
            "faq_question": "How should I utilize Hyaluronic acid?",
            "faq_answers": "Use a moisturizer infused with hyaluronic acid at the time when you'd usually moisturize. Ideally, this would be twice a day and always after cleansing, exfoliating, or applying serums."
          }
        }
      },
