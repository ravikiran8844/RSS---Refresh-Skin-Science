        "dc214157-7ac2-418e-b312-24fdfa67143d": {
          "type": "faq-sec-accordion",
          "settings": {
            "faq_question": "Is it necessary to do a patch test before applying?",
            "faq_answers": "Yes, it is recommended to do a patch test before applying serum, irrespective of the skin type."
          }
        },
        "9d8a1203-a77b-4c87-9856-a059c46fc1a6": {
          "type": "faq-sec-accordion",
          "settings": {
            "faq_question": "Will my skin feel Oily after application?",
            "faq_answers": "Our lightweight, oil-free formula ensures your skin feels light and supple post-application without leaving a sticky residue on the face."
          }
        },
        "239f1633-81a0-4611-84fe-fe2785adf23a": {
          "type": "faq-sec-accordion",
          "settings": {
            "faq_question": "Can I use these along with my Vitamin C serum?",
            "faq_answers": "If topical Vitamin C is used as a part of the skincare routine, it is always recommended that you use Niacinamide alternatively with Vitamin C."
          }
        },
        "82175e56-52c3-4f57-a058-a818a6b04b41": {
          "type": "faq-sec-accordion",
          "settings": {
            "faq_question": "Do the products contain SLS\/Paraben?",
            "faq_answers": "All our products are made with clinically proven ingredients formulated by leading experts. You can be rest assured that if it does not belong on your skin, we will not add it there!"
          }
        },
        "f9f6c8d8-ab13-481a-9821-4e2a6c08019d": {
          "type": "faq-sec-accordion",
          "settings": {
            "faq_question": "What is the use of green tea extract and tea oil extract in Salicylic face serum?",
            "faq_answers": "Both these ingredients have excellent antibacterial and anti-inflammatory properties and help soothe the skin and help minimise ten signs of aging."
          }
        }
