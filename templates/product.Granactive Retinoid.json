    "product-faq": {
      "type": "product-faq",
      "blocks": {
        "7d009913-62e9-4feb-8830-e06312699731": {
          "type": "faq-sec-accordion",
          "settings": {
            "faq_question": "Are Retinol and Retinoids the same thing?",
            "faq_answers": "No, they are not the same ingredients. Retinoid is a generic term for all products that contain derivatives of Vitamin A, and Retinol is a type of Retinoid."
          }
        },
        "13b80d15-b19a-4834-90f9-0fe936a408b2": {
          "type": "faq-sec-accordion",
          "settings": {
            "faq_question": "How does Retinoid work?",
            "faq_answers": "Retinoid is a Vitamin A derivative that affects the behavior of aging cells. It promotes healthy cell turnover to improve skin texture, luminosity, and anti-aging effects. It is scientifically validated and a safe substance that improves skin texture, evens skin tone, reduces pigmentation, and prevents acne flare-ups."
          }
        },
        "de17f659-ad7a-4e80-8887-42ce89bf4412": {
          "type": "faq-sec-accordion",
          "settings": {
            "faq_question": "Should I use an SPF post application?",
            "faq_answers": "Yes, always follow up a skincare routine containing active ingredients with SPF to avoid as plenty of active ingredients can increase your sensitivity to UV rays."
          }
        },
        "69c9a38c-e6a6-439a-ae7f-985680ec1457": {
          "type": "faq-sec-accordion",
          "settings": {
            "faq_question": "Which is better - Granactive Retinoid or Retinoids?",
            "faq_answers": "Granactive Retinoid shows ten times more stability than Retinoid and is far more effective and safer on sensitive skin"
          }
        },
        "965fe1c9-79e2-4708-9a4f-b1a408374137": {
          "type": "faq-sec-accordion",
          "settings": {
            "faq_question": "Will using the serum cause any side effects?",
            "faq_answers": "Refresh’s skin serums are made with skin safe materials and are all dermatologist-approved formulations made without any clinically proven side effects."
          }
        }
      },
