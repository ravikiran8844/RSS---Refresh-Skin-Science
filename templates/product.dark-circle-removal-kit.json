        "ef1debd4-2b84-4f2d-8525-42f64e82f94e": {
          "type": "faq-sec-accordion",
          "settings": {
            "faq_question": "Why peptides for dark circles?",
            "faq_answers": "Peptides are the building blocks of collagen, which helps keep your skin firm and smooth. As you age, the rate of collagen your skin produces slows down, so using our under eye roll on with peptides helps stimulate their growth."
          }
        },
        "3735a20f-bc39-43b0-a779-611d98307c5c": {
          "type": "faq-sec-accordion",
          "settings": {
            "faq_question": "Will the kit help with other parts of my face as well?",
            "faq_answers": "Yes, Our kit comes with 20% Vitamin C Serum and Moisturiser with ceramides to work on your skin and provide it a healthy glow."
          }
        },
        "5f3ca5ea-ed3e-4bef-805b-a8be5fff3fab": {
          "type": "faq-sec-accordion",
          "settings": {
            "faq_question": "How often should I use the kit?",
            "faq_answers": "You can follow the regimen given below the kit for effective results. You can also customise your regimen according to your skin type & needs."
          }
        },
        "d5062d9d-3a6e-4e27-8b6a-1879e86346cc": {
          "type": "faq-sec-accordion",
          "settings": {
            "faq_question": "Which skin type is the regime suitable for?",
            "faq_answers": "Our kit is suitable for all skin types as each product is designed specifically to target an issue rather than work in general on your skin"
          }
        },
        "7ac3a2e1-3c5b-4edb-a4a6-c889a5d03c6e": {
          "type": "faq-sec-accordion",
          "settings": {
            "faq_question": "Is it free from harmful chemicals?",
            "faq_answers": "Our products do not contain any substances that might harm your skin. All our ingredients are clinically tested and studies show that 99% of the people do not experience any side effects."
          }
        }
